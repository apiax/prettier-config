{
  "name": "@apiax/prettier-config",
  "version": "1.0.0",
  "description": "Apiax Shareable Prettier Configuration",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apiax/prettier-config.git"
  },
  "keywords": [
    "prettier"
  ],
  "author": "Rafael Gomes <rafael.gomes@apiax.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apiax/prettier-config/issues"
  },
  "homepage": "https://github.com/apiax/prettier-config#readme",
  "devDependencies": {
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3"
  },
  "peerDependencies": {
    "prettier": "3.3.3",
    "prettier-plugin-java": "2.6.4",
    "prettier-plugin-organize-imports": "4.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": false
    },
    "prettier-plugin-java": {
      "optional": true
    },
    "prettier-plugin-organize-imports": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.{json,js,jsx,cjs,mjs,ts,tsx,html,css,scss}": "npx prettier --write"
  }
}